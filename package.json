{
  "name": "resynciq-compliance-engine",
  "version": "1.0.0",
  "description": "AI-powered DRP compliance validation engine",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npm run build:ts && webpack --config config/webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docs": "jsdoc -c jsdoc.json",
    "deploy:staging": "NODE_ENV=staging node scripts/deploy.js",
    "deploy:production": "NODE_ENV=production node scripts/deploy.js",
    "security-audit": "npm audit && snyk test",
    "dependency-check": "depcheck",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "build:ts": "tsc",
    "type-check:watch": "tsc --watch",
    "prestart": "npm run build:ts",
    "predev": "npm run build:ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "axios": "^1.6.2",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "husky": "^8.0.0",
    "snyk": "^1.1234.0"
  },
  "author": "Ali Jakvani",
  "license": "UNLICENSED",
  "private": true
} 